mentions = (
    df_matched.groupby("album_base_title")["web_url"]
    .nunique()
    .reset_index(name="nyt_article_count")
    .sort_values("nyt_article_count", ascending=False)
)

print("=" * 60)
print("NYT Article Mentions by Album:")
print("=" * 60)
print(mentions.to_string(index=False))

